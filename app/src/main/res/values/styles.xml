<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="main_activity_button_style" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/main_button_height</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/main_text_size</item>
        <item name="iconGravity">textStart</item>
        <item name="cornerRadius">@dimen/main_button_corner_radius</item>
        <item name="iconPadding">@dimen/main_padding_left_image</item>
        <item name="iconTint">@color/black</item>
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="android:stateListAnimator">@null</item>
    </style>
    <style name="main_activity_linear_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="settings_activity_linear_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/white</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="settings_activity_text_style">
        <item name="android:layout_width">@dimen/main_button_height</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/settings_text_size</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">@dimen/settings_padding_vertical_and_left_text</item>
        <item name="android:paddingVertical">@dimen/settings_padding_vertical_and_left_text</item>
        <item name="fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="settings_activity_image_button_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="tint">@color/YP_gray</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:paddingLeft">@dimen/main_padding_left_image</item>
        <item name="android:paddingVertical">@dimen/settings_padding_vertical_and_left_text</item>
    </style>
    <style name="settings_activity_linear_layout_style_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="settings_activity_linear_layout_style_top_image_and_text">////
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="backgroundTint">@color/white</item>
    </style>
    <style name="settings_activity_linear_layout_style_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="search_activity_frame_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/round_corners_10</item>
    </style>
    <style name="search_activity_edit_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:drawableTint">@color/YP_gray</item>
    </style>
    <style name="search_activity_image_buttom">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="tint">@color/YP_gray</item>
        <item name="android:layout_gravity">end|center</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="track_view_artist_name_and_track_time_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/track_artist_name_and_track_time_text_size</item>
        <item name="android:textColor">@color/YP_gray</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="drawableTint">@color/YP_gray</item>
    </style>
    <style name="track_view_track_name_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/track_name_text_size</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="no_connect_view_text_view">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/settings_text_size</item>
    </style>
    <style name="no_connect_view_image_view">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:src">@drawable/interneterrorlight</item>
    </style>
    <style name="no_connect_view_update_button" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="backgroundTint">@color/black</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="track_view_image_view">
        <item name="android:background">@android:color/transparent</item>
        <item name="tint">@color/YP_gray</item>
    </style>
    <style name="main_text_view_audio_player_activity">
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="additional_main_text_view_audio_player_activity">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/additional_text_size_15</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="constraint_layout_audio_player_activity">
        <item name="android:background">@color/white</item>
    </style>
    <style name="based_text_view_audio_player_activity">
        <item name="android:textColor">@color/YP_gray</item>
        <item name="android:textSize">@dimen/additional_text_size_15</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:layout_marginBottom">@dimen/margin_17</item>
    </style>
    <style name="custom_text_tab">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="tab_layout_mediatech_activity">
        <item name="tabTextColor">@color/black</item>
        <item name="tabIndicatorColor">@color/black</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="tabSelectedTextColor">@color/black</item>
    </style>
    <style name="empty_list_of_favorite_track_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/emptysearchresultlight</item>
    </style>
    <style name="text_empty_list_fragment_favorite_track">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="nav_button_style">
        <item name="android:background">@color/white</item>
        <item name="itemTextColor">@drawable/nav_view_item</item>
        <item name="itemIconTint">@drawable/nav_view_item</item>
    </style>
    <style name="player_activity_image_button">
        <item name="backgroundTint">@color/white</item>
    </style>

    <style name="text_layout_add_playlist_fragment"
        parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
    </style>

    <style name="playlist_view_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/track_artist_name_and_track_time_text_size</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="drawableTint">@color/black</item>
    </style>
    <style name="artwork_playlist_image_view">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:tint">@color/YP_gray</item>
    </style>
    <style name="bottom_sheet">
        <item name="background">@color/white</item>
    </style>
</resources>